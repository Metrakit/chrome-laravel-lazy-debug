{
  "name": "Laravel Lazy Debug",
  "version": "1.0.0",
  "description": "A Laravel Lazy debugger Chrome extension",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Laravel Lazy Debug",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
